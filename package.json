{
  "name": "akita-filters-plugin",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4949",
    "build": "ng build --prod --output-path docs --base-href /akita-filters-plugin/",
    "compile": "ng build akita-filters-plugin --prod",
    "compile:datasource": "ng build akita-mat-datasource --prod",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release",
    "beta": "semantic-release -d"
  },
  "author": "Emmanuel DE SAINT STEBAN",
  "keywords": [
    "angular",
    "ngx",
    "akita",
    "akitajs",
    "filters",
    "state management"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "rootDir": "projects",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^akita-filters-plugin$":
        "<rootDir>/akita-filters-plugin/src/lib/index.ts"
      ,
      "^akita-filters-plugin\/datasource$":
        "<rootDir>/akita-mat-datasource/src/lib/index.ts"

    },
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "verbose": true,
    "testRegex": ".*/test/.*\\.spec.(ts|js)$"
  },
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/cdk": "~11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/material": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@datorama/akita": "^6.0.0",
    "@datorama/akita-ng-entity-service": "^6.0.0",
    "@types/jest": "^26.0.20",
    "core-js": "^3.8.3",
    "ngx-markdown": "^11.0.1",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/language-service": "~11.2.0",
    "@datorama/akita-ng-router-store": "^6.0.0",
    "@datorama/akita-ngdevtools": "^6.0.0",
    "@jest/globals": "^26.6.2",
    "@types/jasmine": "~3.6.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.6",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.1",
    "protractor": "~7.0.0",
    "semantic-release": "^17.3.8",
    "ts-jest": "^26.5.1",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manudss/akita-filters-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/manudss/akita-filters-plugin/issues"
  },
  "homepage": "https://manudss.github.io/akita-filters-plugin/",
  "license": "Apache-2.0",
  "release": {
    "pkgRoot": "dist/akita-filters-plugin",
    "branches": [
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": "beta"
      }
    ],
    "prepare": {
      "pkgRoot": "dist/akita-filters-plugin"
    },
    "publish": {
      "pkgRoot": "dist/akita-filters-plugin"
    }
  },
  "prepare": {
    "pkgRoot": "dist/akita-filters-plugin"
  },
  "publish": {
    "pkgRoot": "dist/akita-filters-plugin"
  }
}
